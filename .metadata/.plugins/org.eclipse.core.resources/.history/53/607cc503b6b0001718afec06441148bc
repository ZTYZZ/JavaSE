package cn.ztyzz_01;

import java.util.Scanner;

/*
 * 需求:反转字符串
 * 例:
 * 输入:abc
 * 输出:cba
 */
public class ReverseString {
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		String s = in.nextLine();
		// 倒序遍历的两种方法
		// 第一种方法charAt()
		for(int i = s.length()-1;i>=0;i--) {
			System.out.print(s.charAt(i));
		}
	}
}
